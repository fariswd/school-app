<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teachers</title>
  </head>
  <body>
    <h1>Teachers Data</h1>
    <table border="1">
      <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last name</th>
        <th>Email</th>
      </tr>
      <% teachers.forEach(t=>{ %>
        <tr>
          <td><%= t.id %></td>
          <td><%= t.first_name %></td>
          <td><%= t.last_name %></td>
          <td><%= t.email %></td>
        </tr>
      <% }) %>
    </table>
  </body>
</html>